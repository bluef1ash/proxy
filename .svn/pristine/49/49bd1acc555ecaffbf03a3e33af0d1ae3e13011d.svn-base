@charset "utf-8";
@import url("public.css");
/*头部开始*/
.header {
	height: auto;
	margin: 1px auto 10px auto;
	overflow: hidden;
}
/*注册与登录信息栏开始*/
.header .top-bar-right {
	color: #336699;
	height: auto;
	width: 100%;
	float: right;
	overflow: hidden;
	text-align: right;
	background-color: #F2F9F9;
	margin: 0px 5px;
	font-family: "Arial", "Helvetica", "sans-serif";
	font-size: 14px;
	line-height: 200%;
}
.header .top-bar-right a {
	color:#336699;
}
.header .top-bar-right a:hover {
	background:none repeat scroll 0 0 #003399;
	color:#FFF;
	text-decoration:none;
}
.header .top-bar-right #register {
	float: right;
	margin-right: 30px;
}
.header .top-bar-right #login {
	float: right;
}
/*注册与登录信息栏结束*/
/*注册与登录弹出框开始*/
/*注册框开始*/
#register {
	width: 530px;
	height: auto;
	overflow: hidden;
	position: absolute;
	z-index: 999;
	background: #fff;
	left: 2px;
	top: 15px;
	display: none;
	text-align: center;
}
#register .reg-title {
	background: url("../images/reg-bg.png") repeat-x;
    height: 45px;
}
#register .reg-title p {
	color: #666666;
    float: left;
    font-family: 'Microsoft YaHei';
    font-size: 19px;
    line-height: 45px;
    padding-left: 27px;
}
#register .reg-title a {
	display: inline-block;
    float: right;
    height: 26px;
    margin: 10px 10px 0 0;
    width: 33px;
    background-position: -878px -209px;
}
#register .reg-title a:hover {
	background-position: -878px -239px;
}
#reg-wrap{
	width: 530px;
	height: auto;
	overflow: hidden;
}
#reg-wrap .reg-left {
	background: url("../images/reg-left.png") repeat-y;
    color: #666666;
    float: left;
    font-family: "微软雅黑";
    font-size: 16px;
    padding-top: 20px;
    width: 157px;
    float: left;
}
#reg-wrap .reg-left ul li {
	background-position: -2px -280px;
    height: 52px;
    line-height: 52px;
    width: 157px;
}
#reg-wrap .reg-left ul li span {
	color: #fff;
    background-position: -877px -38px;
    display: inline-block;
    height: 23px;
    line-height: 26px;
    padding-left: 30px;
}
#reg-wrap .reg-left .reg-l-bottom {
	text-align: center;
	font-family: "宋体";
    font-size: 14px;
    margin: 275px 0 30px 0;
}
#reg-wrap .reg-left .reg-l-bottom a {
	text-decoration: none;
	color: #0082CB;
	padding-right: 5px;
}
#register .reg-right{
	width: 370px;
	height: auto;
	overflow: hidden;
	float: right;
	padding-top: 16px;
}
#register .reg-right ul li {
	color: #999999;
    font-family: "arial","宋体","sans-serif";
	height: 62px;
	margin-bottom: 10px;
	margin-right: 20px;
	text-align: left;
}
#register .reg-right ul li label {
	color: #000000;
    display: inline-block;
    font-size: 14px;
    height: 41px;
    line-height: 41px;
    padding-right: 10px;
    text-align: right;
    width: 60px;
}
#register .reg-right ul li input {
    line-height: 38px;
    width: 230px;
    /*background: url(btn.png) no-repeat -2px -159px;*/
}
#register .reg-right ul li span {
	display: block;
	text-align: left;
	margin-left: 80px;
    word-break: break-all;
    word-wrap: break-word;
}
#register .reg-right ul li #reg-verify {
    line-height: 43px;
    width: 110px;
    /*height: 43px;
    background: url(btn.png) no-repeat -665px 0;*/
}
#register .reg-right ul li #verify-img {
	float: right;
    _float:none;
}
#register .reg-right ul .submit {
	height:62px;
}
#register .reg-right ul .submit input {
	border: 0 none;
    color: #FFFFFF;
    display: inline-block;
    font-family: "微软雅黑";
    font-size: 16px;
    height: 38px;
    margin: 10px 0 0 69px;
    outline: medium none;
    width: 269px;
    background-position: -268px -2px;
}
#register .reg-right ul .submit .reg-btn-cur {
	background-position: -268px -63px;
}
/*注册框结束*/
/*登录框开始*/
#login {
	width: 460px;
	height: auto;
	overflow: hidden;
	position: absolute;
	z-index: 999;
	background: #fff;
	display: none;
	text-align: center;
}
#login .login-form{
	height: auto;
	overflow: hidden;
	width: 460px;
}
#login .login-form #login-msg {
	display: block;
    height: 24px;
    line-height: 24px;
    color: #f00;
}
#login .login-title {
	background-position: 0px -167px;
	width: 460px;
    height: 56px
}
#login .login-title p {
	height: 38px;
	padding-top: 18px;
	margin-left: 10px;
	float: left;
	color: #666666;
	font: 16px/1.5 'microsoft yahei';
}
#login .login-title a {
	background-position: -219px -130px;
    display: block;
    float: right;
    height: 24px;
    width: 29px;
    margin: 15px 10px 0px 0px;
}
#login .login-title a:hover {
	background-position: -262px -130px;
}
#login .login-form li {
	margin: 10px 0;
}
#login .login-form li label{
	color: #000000;
    font-size: 14px;
    height: 41px;
    line-height: 41px;
    padding-right: 10px;
    text-align: right;
    width: 60px;
    font-family: "arial","宋体","sans-serif";
}
#login .login-form li .input {
    line-height: 38px;
    width: 250px;
    /*padding-left: 5px;
    border: none;
	height: 38px;
    background: url(btn.png) no-repeat scroll -2px -159px;*/
    font: 14px/20px "arial","simsun";
}
#login .login-form .login-auto {
	text-align: left;
}
#login .login-form .login-auto label {
	font-size: 12px;
    height: 30px;
    line-height: 30px;
    text-align: left;
    vertical-align: middle;
    width: 123px;
    color: #000000;
    margin-left: 115px;
    _float:left;
}
#regis-now {
    display: inline-block;
    height: 24px;
    line-height: 24px;
    text-decoration: none;
    margin-right: 80px;
    margin-top: 2px;
    float: right;
    _display : block;
    _margin-left:70px;
}
#login-btn {
	border: 0 none;
    height: 39px;
    margin-left: 35px;
    width: 266px;
    background-position: 0px -2px;
    margin-bottom: 20px;
}
#login .login-form li .login-btn-cur {
	background-position: 0 -63px;
}
/*登录框结束*/
/*背景遮罩*/
#background {
    width : 100%;
    position : absolute;
    top : 0;
    left : 0;
    z-index : 998;
    background-color : #000000;

}
/*背景遮罩结束*/
/*采集页LOGO及输入框开始*/
.header .title {
	line-height: 1.8em;
	clear: both;
	height: auto;
	margin-top: 10px;
	overflow: hidden;
	text-align: center;
}
/*LOGO图开始*/
.header .title .logo {
	float: left;
	z-index: auto;
	height: auto;
	width: 40%;
	text-align: left;
}
/*LOGO图结束*/
/*采集功能开始*/
.header .title .collect_input {
	float: left;
	overflow: hidden;
	height: auto;
	text-align: left;
	padding-top: 30px;
}
.header .title .collect_input form #kw {
	font-size: 16px;
    height: 20px;
	width: 370px;
    padding-top: 2px;
}
.header .title .collect_input form a {
	display: block;
	float:right;
	color: #fff;
    font-size: 14px;
	width:70px;
	text-align:center;
    height: 27px;
    padding-top: 2px;
	margin:3px 10px;
	text-decoration: none;
}
.header .title .collect_input .text {
	font: 14px/150% "Arial","Helvetica","sans-serif";
	color: #333;
	margin-bottom: 10px;
}
.header .title .collect_input form .dropdown {
	float: left;
}
/*采集更多功能开始*/
.header .title .collect_input .update_way {
	margin-top: 10px;
}
.header .title .collect_input .update_way li {
	float:left;
}
.header .title .collect_input .update_way .collection {
	width: 300px;
	overflow: hidden;
	height: auto;
	margin-left: 20px;
}
.header .title .collect_input .update_way .collection dt #delete_tmp {
	font-size: 12px;
	line-height: 1.8em;
	color: #CCC;
}
.header .title .collect_input form .update_way .collection #collection_type {
	display: none;
}
/*采集更多功能结束*/
/*采集功能结束*/
/*导航条开始*/
.nav-srh {
	height: 38px;
	padding-left: 5px;
	width: 600px;
	margin: 0 auto;
}
.nav-srh input {
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #A6D098;
    float: left;
    height: 26px;
    line-height: 26px;
    width: 400px;
}
.navPanel {
    background-color: #666666;
    background-image: url("../images/bg_mid.png");
    background-position: left top;
    background-repeat: repeat-x;
    border-bottom: 1px solid #999999;
    border-top: 1px solid #999999;
    color: #666666;
    line-height: 2em;
}
.navPanel .innerNavPanel {
	width: 900px;
	margin: 0px auto;
	height: auto;
	overflow: hidden;
	padding: 0px;
}
.navPanel .innerNavPanel li {
	float: left;
	height: 32px;
    border-left: 1px none #333333;
    border-right-color: #666666;
    border-right-style: none;
    line-height: 32px;
    white-space: nowrap;
}
.navPanel .innerNavPanel li a {
    border-left: 1px solid #999999;
    border-right: 1px none #999999;
    color: #000000;
    display: block;
    padding: 0px 20px;
    text-decoration: none;
}
.navPanel .innerNavPanel li a:visited,.navPanel .innerNavPanel li a:active {
    background-color: #666666;
    border-left: 1px solid #666666;
    border-right: 1px solid #666666;
    color: #645A44;
    padding: 0px 20px;
}
.navPanel .innerNavPanel li a:hover {
    background-color: #666666;
    background-image: url("../images/bg_btn_active.png");
    background-position: left top;
    background-repeat: repeat-x;
    color: #FFFFFF;
}
.navPanel .innerNavPanel li .weibo{
	padding: 0px 30px;
	background: url(../images/weibo-mini.png) 7px 8px no-repeat;
}
/*导航条结束*/
/*头部结束*/
/*主体开始*/
.main {
	margin: 0px auto 10px auto;
	height: auto;
	text-align: center;
	overflow: hidden;
	width: 100%;
}
#location{
    width: 980px;
    height: 28px;
    line-height: 28px;
    margin: 0 auto;
    margin-top: 10px;
    text-align: left;
}
/*首页采集功能开始*/
.main #kw {
	cursor: url("../images/mouse/droplet_busy.ani"), text;
	color: #1C1C1C;
    font-size: 16px;
    height: 18px;
    padding-top: 2px;
	padding-left: 0px;
	margin:5px 10px;
	line-height: 18px;
}
.main .nav-srh form .dropdown {
	margin-top: 8px;
	float: left;
	margin-left: 20px;
}
.main .nav-srh form .dropdown #menu1 li .way li {
	float: left;
}
.main .nav-srh form .dropdown #menu1 input {
	width: auto;
}
.main .nav-srh form .dropdown #menu1 li label {
	font-weight: normal;
	margin: 0px;
}
.main .nav-srh form .dropdown #menu1 li {
	margin-bottom: 10px;
}
.main .nav-srh form .dropdown #menu1 #collection_type {
	display: none;
}
.main .nav-srh form .update_way li label input {
	height: auto;
	width: auto;
}
.main .nav-srh form .update_way {
	clear: both;
}
.main form .update_way {
	clear: both;
	overflow: hidden;
	width: 500px;
	margin-right: auto;
	margin-left: auto;
	margin-top: 10px;
}
.main form div .update_way li {
	float: left;
}
.main form div .update_way .collection #collection_type {
	float: left;
}
.main form .update_way li {
	float: left;
	margin: 0px 10px;
}
.main form .update_way .collection #collection_type {
	float: left;
	display: none;
}
.main form .update_way .collection {
	width: 37%;
}
.main .nav-srh a:hover {
	text-decoration: none;
}
.main .nav-srh #re {
    background-position: 0px -100px;
    font-size: 13px;
    height: 29px;
    padding: 1px 0 3px 3px;
    text-align: center;
	color:#000;
    width: 54px;
	line-height:29px;
	margin:5px 10px;
	display: block;
	float: left;
}
.main .nav-srh .bn-srh {
    background-position: 0px -100px;
    border: medium none;
    font-size: 13px;
    height: 29px;
    padding: 1px 0 3px 3px;
    text-align: center;
    width: 54px;
	margin:5px 0px;
}
/*首页采集功能结束*/
/*提示功能开始*/
.main .about {
	clear:both;
}
.main .about .gn {
	color: #666;
	text-align: center;
	margin-right: auto;
	margin-left: auto;
	width: 350px;
}
.main .about .gn a {
	color: #666;
}
.main .about .gn a:hover {
	color: #666;
	text-decoration:none;
}
.main .about .gn li {
	float: left;
	line-height: 4em;
	padding-right: 25px;
	padding-left: 25px;
}
.main .about .sitefx {
	clear: both;
	line-height: 1.8em;
}
.main .about .sitefx a {
	color: #336699;
}
.main .about .sitefx a:hover {
	background:none repeat scroll 0 0 #003399;
	color:#FFF;
	text-decoration:none;
}
.main .about .videotype {
	width:780px;
	margin-right: auto;
	margin-left: auto;
	height:200px;
	border:solid 1px #000;
}
.main .about .videotype .table_caption {
	font-size: 14px;
	line-height: 1.2em;
	color: #333;
	text-align: left;
}
.main .about .videotype td {
	font-size: 14px;
	line-height:1.2em;
	text-align: center;
	width:20%;
}
.main .about .videotype td a {
	color:#666;
}
/*提示功能结束*/
/*视频名称显示开始*/
.main .video_info {
	line-height: 2.5em;
	color: #333;
	font: 14px/150% "Arial","Helvetica","sans-serif";
	text-align: left;
	background-color: #F2F9F9;
	background-repeat: repeat;
	background-position: left top;
}
.main .video_info span {
	font-weight: bold;
}
/*视频显示结束*/
/*功能按钮开始*/
.main .buttons {
	width: 386px;
	text-align: center;
	margin:10px auto;
	height: auto;
	overflow: hidden;
}
.main .buttons li{
	float: left;
	padding: 0px 30px;
}
.main .buttons #upload {
	clear:both;
	line-height:2.5em;
}
.main .buttons li ul li {
	margin: 5px 5px;
}
.sub {
	background: #7094B8;
    border-color: #CCCCFF #CCCCCC #CCCCCC #CCCCFF;
    border-width: thin;
    color: #FFFFFF;
    font-size: 12px;
    height: 28px;
	padding:0px 10px;
}
/*功能按钮结束*/
/*上传开始*/
#category {
	width: 480px;
	height: auto;
	overflow: hidden;
	position: absolute;
	z-index: 999;
	display: none;
	background: #fff;
}
#category .title {
	margin: 0px;
	width: 480px;
	height: 32px;
	line-height: 32px;
	border-bottom: 1px solid #E0E0E0;
	background: #FAFAFA;
}
#category .title span {
	display: block;
	width: 120px;
	height: 32px;
	line-height: 32px;
	font-size: 14px;
	font-weight: bold;
	color: #666666;
	float: left;
}
#category .title .close-window {
	display: block;
	border: 0px;
	padding: 0px;
	background: url("../images/close.png");
    height: 25px;
    float: right;
    width: 25px;
    margin-top: 3px;
    margin-right: 10px;
}
#category .title .close-window:hover{
	background-position: -25px 0;
}
#category .sel p {
	clear: both;
	width: 480px;
	height: 60px;
	line-height: 60px;
	text-align: left;
	text-indent: 10px;
}
#category .sel .vname {
	width: 360px;
	height: auto;
	margin-top: 10px;
	margin-bottom: 5px;
	margin-left: auto;
	margin-right: auto;
	line-height: 2em;
}
#category .sel .vname label {
	display: block;
}
#category .sel select {
	width: 150px;
	height: 230px;
	float: left;
	margin-left: 10px;
	border: none;
}
#category .sel select option {
	font-size: 14px;
	padding: 2px;
}
.main #upload ul #movieClassify, .main #upload ul #tvsClassify, .main #upload ul #entertainmentClassify {
	display: none;
}
#category .bottom{
	height: 50px;
	background:#F8F8F8;
}
#category .bottom #ok {
	display: block;
	float: right;
	width: 70px;
	height: 30px;
	line-height: 30px;
	border: 1px solid #3EAF0E;
	background: #4CC311;
	border-radius: 4px;
	color: #fff;
	font-weight: bold;
	font-size: 14px;
	margin-top: 10px;
	margin-right: 10px;
}
/*上传结束*/
/*显示XML开始*/
.main #lists {
	overflow: hidden;
	height: auto;
	text-align: left;
	font:12px;
	margin: 10px auto;
}
.main #lists #editxml {
	margin:0px 10px;
	width: 98%;
}
.main .site {
	text-align: left;
}
.main .site li {
	margin: 10px 0px;
}
/*显示XML结束*/
.main .buttons ul li {
	float: left;
	margin: 0px 20px;
}
.footer {
	text-align: center;
	margin-top: 10px;
}